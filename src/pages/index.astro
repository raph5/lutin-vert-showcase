<style lang="scss">
@import '../scss/var';

.banner {
  width: 100%;
  height: calc(100vh - var(--header-height));
  background: linear-gradient(0deg, rgba(30, 24, 17, 0.7), rgba(30, 24, 17, 0.7)), url('/assets/banner-phone.jpg');
  background-size: cover;
  background-position-x: center;
  background-position-y: 20%;
  display: flex;
  align-items: center;
  flex-direction: column;
  color: $color-font-white;

  &__title {
    font-family: $font-wide;
    margin-bottom: 0;
    margin-top: 30vh;
    font-size: 38px;
    font-weight: bold;
    text-align: center;
  }
  &__subtitle {
    font-family: $font-wide;
    font-weight: lighter;
    font-size: 20px;
    text-align: center;
    margin-top: 15px;
  }
  &__cta {
    margin-top: auto;
    margin-bottom: 29px;
    color: $color-font-white;
    text-align: center;
    position: relative;
    text-decoration: none;
    font-size: 13px;
    font-weight: bold;
    letter-spacing: 1px;

    &::after {
      content: '';
      position: absolute;
      width: 2px;
      height: 25px;
      left: calc(50% - 1px);
      top: calc(100% + 4px);
      background-color: $color-font-white;
    }
  }

  @media (min-width: 700px) {
    background: linear-gradient(0deg, rgba(30, 24, 17, 0.7), rgba(30, 24, 17, 0.7)), url('/assets/banner-pc.jpg');
    background-size: cover;
    background-position-x: center;
    background-position-y: 20%;
  
    &__title {
      font-size: 70px;
    }
    &__subtitle {
      font-size: 25px;
    }
  }
}

.qui-sommes-nous {
  color: $color-secondary-brown;
}

.atelier {
  color: $color-font-white;
  background: url('/assets/texture.svg'), linear-gradient(0deg, $color-primary-regular, $color-primary-regular);
  padding-bottom: 400px;
  position: relative;

  .flower-1 {
    position: absolute;
    width: 207px;
    height: 144px;
    padding-top: 60px;
    right: 20px;
    bottom: 125px;
    
    span {
      position: relative;
      z-index: 1;
      display: block;
      margin: 0 auto;
    }
    &__num {
      font-family: $font-wide;
      font-weight: bold;
      font-size: 32px;
      width: max-content;
      word-spacing: -0.4em;
    }
    &__title {
      font-size: 14px;
      max-width: 80px;
      text-align: center;
    }
    svg {
      position: absolute;
      z-index: 0;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
    }
  }
  .flower-2 {
    position: absolute;
    left: -100px;
    bottom: -70px;
  }
}

.chantier {
  color: $color-secondary-brown;
  position: relative;
  padding-bottom: 200px;
  overflow: hidden;

  button {
    position: relative;
    margin-top: 20px;
    border: none;
    background: none;
    padding: 0;
    
    a {
      display: block;
      border-radius: 500px;
      padding: 9px 26px;
      font-size: 12px;
      line-height: 18px;
      font-family: $font-wide;
      font-weight: bold;
      background-color: #D2C1BC;
      position: relative;
      z-index: 2;
      margin: 0;
      text-decoration: none;
      color: $color-secondary-brown;
    }
  }

  svg {
    position: absolute;
    bottom: -1px;
  }
  .box-1 {
    left: 60px;
  }
  .box-2 {
    left: 190px;
  }

  @media (min-width: 700px) {
    button::after {
      content: '';
      position: absolute;
      z-index: 1;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      border-radius: 500px;
      background-color: $color-secondary-brown;
      transition: transform 80ms;
    }
    button:hover::after {
      transform: translate(3px, 3px);
    }
